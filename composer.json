{
  "name": "zrcms/rcm-compatibility",
  "description": "ZRCMS to RCM Compatibility Adapter",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "reliv",
    "content",
    "manager",
    "zend",
    "expressive",
    "middleware",
    "module"
  ],
  "homepage": "http://www.reliv.com/",
  "authors": [
    {
      "name": "James Jervis",
      "email": "jjervis@relivinc.com",
      "homepage": "http://www.reliv.com/"
    },
    {
      "name": "Rod McNew",
      "email": "rmcnew@relivinc.com",
      "homepage": "http://www.reliv.com/"
    }
  ],
  "require": {
    "php": ">=7",
    "psr/container": "^1",
    "psr/http-message": "^1",
    "reliv/zf-config-factories": "^4",
    "zendframework/zend-expressive": "1.* || 2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "ZrcmsRcmCompatibility\\": "/",
      "ZrcmsRcmCompatibility\\RcmAdapter\\": "rcm-adapter/",
      "ZrcmsRcmCompatibility\\Compatibility\\": "compatibility/",
      "ZrcmsRcmCompatibility\\RcmDisabler\\": "rcm-disabler/",
      "ZrcmsRcmCompatibility\\Rcm\\": "rcm/"
    }
  },
  "suggest": {
    "zendframework/zend-diactoros": "Should install with Expressive"
  }
}
